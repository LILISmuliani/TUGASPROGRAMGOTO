#include <iostream>
#include <string>
using namespace std;
// TUGAS PRAKTIKUM DIRUMAH 07/04/2020
int main ()
{
	cout << " Nama : Lilis Muliani " << endl;
	cout << " Nim : ETE10190097 " << endl;
	cout << " Program Studi : Teknik Elektromedik " << endl;
	cout << " =================================== " << endl;
	// membuat program goto
	char jump = 'R' ;
	
	cout << " Masukkan huruf o untuk melompat " << endl;
	cout << " Masukkan huruf z untuk tidak melompat " << endl;
	cin >> jump;
	
	cout << 'A' << endl;
	if ( jump == 'o') goto lompat;
	cout << "N" << endl;
	lompat:
	cout << "S" << endl;
	
	return 0; 
	
}
